{
  "expo": {
    "name": "Metacritic",
    "slug": "Metacritic",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/metacritic_icon.png",
    "userInterfaceStyle": "light",
    "scheme": "metacritic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "transparent"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/metacritic_icon.png",
        "backgroundColor": "transparent"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
